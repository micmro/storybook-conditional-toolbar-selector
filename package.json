{
  "name": "storybook-conditional-toolbar-selector",
  "version": "0.1.0-alpha-4",
  "description": "Helper Storybook addon to define story specific toolbar dropdown to use in custom decorators",
  "main": "dist/preset.js",
  "types": "dist/@types/types",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "keywords": [
    "storybook",
    "addons",
    "menu",
    "toolbar"
  ],
  "author": "Michael Mrowetz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/micmro/storybook-conditional-toolbar-selector.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@storybook/react": "^6.1.14",
    "babel-loader": "^8.2.2",
    "typescript": "^4.1.3",
    "core-js": "^3.8.2"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "NODE_ENV=production rm -rf dist && npm run build:files && npm run build:types",
    "build:files": "babel ./src --out-dir ./dist --extensions .ts --extensions .tsx --ignore src/types.ts",
    "build:types": "rm -rf dist/@types && tsc --noEmit false --emitDeclarationOnly --outDir ./dist/@types",
    "clean": "rm -rf ./node_modules/.cache dist",
    "typecheck": "tsc --noEmit",
    "prepublish": "npm run typecheck && npm run build"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
